              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "${__field.labels.vm_name}"
                }
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "gridPos": {
          "h": 4,
          "w": 24,
          "x": 0,
          "y": 16
        }
      },
      {
