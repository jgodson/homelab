{
  "id": null,
  "title": "VM Disk Usage",
  "tags": ["vm", "disk", "homelab", "ssh", "telegraf"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "vm_name",
          "type": "query",
          "label": "VM Name",
          "multi": true,
          "includeAll": true,
          "allValue": "",
          "query": "SHOW TAG VALUES FROM \"vm_disk_usage\" WITH KEY = \"vm_name\"",
          "datasource": {
            "type": "influxdb",
            "uid": "aegyydl2mv37kd"
          },
          "refresh": 1,
          "regex": "",
          "sort": 1
        },
        {
          "name": "vm_hostname",
          "type": "query", 
          "label": "VM Hostname",
          "multi": true,
          "includeAll": true,
          "allValue": "",
          "query": "SHOW TAG VALUES FROM \"vm_disk_usage\" WITH KEY = \"vm_hostname\"",
          "datasource": {
            "type": "influxdb",
            "uid": "aegyydl2mv37kd"
          },
          "refresh": 1,
          "regex": "",
          "sort": 2
        },
        {
          "name": "vm_ip",
          "type": "query", 
          "label": "VM IP",
          "multi": true,
          "includeAll": true,
          "allValue": "",
          "query": "SHOW TAG VALUES FROM \"vm_disk_usage\" WITH KEY = \"vm_ip\"",
          "datasource": {
            "type": "influxdb",
            "uid": "aegyydl2mv37kd"
          },
          "refresh": 1,
          "regex": "",
          "sort": 3
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "VM Disk Usage Overview",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "influxdb",
              "uid": "aegyydl2mv37kd"
            },
            "query": "SELECT last(\"used_percentage\") FROM \"vm_disk_usage\" WHERE (\"vm_name\" =~ /^$vm_name$/) AND (\"vm_hostname\" =~ /^$vm_hostname$/) AND (\"vm_ip\" =~ /^$vm_ip$/) AND $timeFilter GROUP BY \"vm_name\", \"vm_hostname\"",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 85
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "vm_name"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "${__field.labels.vm_name}"
                }
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Total VMs Monitored",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "influxdb",
              "uid": "aegyydl2mv37kd"
            },
            "query": "SELECT last(\"success\") FROM \"vm_collection_status\" WHERE \"stage\" = 'end' AND $timeFilter",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [
              {
                "options": {
                  "running": {
                    "color": "green",
                    "index": 0,
                    "text": "Running"
                  },
                  "stopped": {
                    "color": "red", 
                    "index": 1,
                    "text": "Stopped"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "short"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "status"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "${__field.labels.status}"
                }
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "gridPos": {
          "h": 4,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Average Disk Usage",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "influxdb",
              "uid": "aegyydl2mv37kd"
            },
            "query": "SELECT mean(\"used_percentage\") FROM \"vm_disk_usage\" WHERE (\"vm_name\" =~ /^$vm_name$/) AND (\"vm_hostname\" =~ /^$vm_hostname$/) AND (\"vm_ip\" =~ /^$vm_ip$/) AND $timeFilter",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 85
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "gridPos": {
          "h": 4,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 4,
        "title": "VM Disk Usage Over Time",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "type": "influxdb",
              "uid": "aegyydl2mv37kd"
            },
            "query": "SELECT mean(\"used_percentage\") FROM \"vm_disk_usage\" WHERE (\"vm_name\" =~ /^$vm_name$/) AND (\"vm_hostname\" =~ /^$vm_hostname$/) AND (\"vm_ip\" =~ /^$vm_ip$/) AND $timeFilter GROUP BY time($__interval), \"vm_name\", \"vm_hostname\" fill(null)",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 0,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": true,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 85
                }
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "${__field.labels.vm_name}"
                }
              ]
            }
          ]
        },
        "options": {
          "legend": {
            "calcs": ["max", "lastNotNull"],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 7,
